<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res/com.yoo_hoo"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.yoo_hoo.DemoActivity" >

    <RelativeLayout
        android:id="@+id/blank1"
        android:layout_width="match_parent"
        android:layout_height="11dp"
        android:background="#95000000" />

    <RelativeLayout
        android:id="@+id/blank2"
        android:layout_width="16dp"
        android:layout_height="match_parent"
        android:layout_below="@+id/blank1"
        android:background="#95000000" />

    <RelativeLayout
        android:id="@+id/blank3"
        android:layout_width="16dp"
        android:layout_height="match_parent"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/blank1"
        android:background="#95000000" />

    <RelativeLayout
        android:id="@+id/blank4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#95000000" />

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="changeInfo"
        android:src="@drawable/circle" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="changeInfo"
        android:src="@drawable/circle1" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="changeInfo"
        android:src="@drawable/circle1" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="changeInfo"
        android:src="@drawable/circle1" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#95000000"
        android:gravity="center_vertical"
        android:text="@string/sAudioFile" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="changeInfo"
        android:src="@drawable/circle1" />

    <RelativeLayout
        android:id="@+id/blank5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#95000000" />

    <RelativeLayout
        android:id="@+id/blank7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#95000000" />


</RelativeLayout>